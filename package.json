{
  "name": "babael-restify-api",
  "version": "0.0.1",
  "description": "RESTful API for primarily handling interactions with Microsoft's Cognitive Services for Facial Recognition",
  "license": "MIT",
  "author": "Damian Nolan",
  "keywords": [
    "restify",
    "es6",
    "node",
    "facial login"
  ],
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/damiannolan/babel-restify-api-v1.git"
  },
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "start": "node server.js",
    "watch": "nodemon --exec npm run babel-node -- src/server.js | bunyan"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "config": "^1.30.0",
    "restify": "^4.3.3",
    "restify-cors-middleware": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "nodemon": "^1.17.3"
  }
}
